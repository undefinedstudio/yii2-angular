{
  "name": "undefinedstudio/yii2-angular",
  "type": "yii2-extension",
  "description": "Yii2 Angular Assets",
  "authors": [
    {
      "name": "Gabriele Della Casa Venturelli",
      "email": "hardsetting@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    },
    {
      "name": "Luca Gatti",
      "email": "luca.horn@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    }
  ],
  "keywords": ["yii2", "angular", "angularjs"],
  "homepage": "https://github.com/undefinedstudio/yii2-angular",
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/angular": "1.4.*",
    "bower-asset/angular-animate": "1.4.*",
    "bower-asset/angular-aria": "1.4.*",
    "bower-asset/angular-material": "1.4.*",
    "bower-asset/angular-mocks": "1.4.*",
    "bower-asset/angular-resource": "1.4.*",
    "bower-asset/angular-route": "1.4.*"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-0": {
      "undefinedstudio\\yii2\\angular\\": "/"
    }
  }
}
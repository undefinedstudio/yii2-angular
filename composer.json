{
  "name": "undefinedstudio/yii2-angular",
  "type": "yii2-extension",
  "description": "Yii2 Angular Assets",
  "version": "0.2.0",
  "authors": [
    {
      "name": "Gabriele Della Casa Venturelli",
      "email": "hardsetting@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    },
    {
      "name": "Luca Gatti",
      "email": "luca.horn@gmail.com",
      "homepage": "http://twistedmirrorgames.com"
    }
  ],
  "keywords": ["yii2", "angular", "angularjs"],
  "homepage": "https://github.com/undefinedstudio/yii2-angular",
  "license": "BSD-3-Clause",
  "require": {
    "yiisoft/yii2": "*"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "undefinedstudio\\yii2\\angular\\": ""
    }
  },
  "suggest": {
    "bower-asset/angular": "Main dependency for AngularJS.",
    "bower-asset/angular-resource": "Shortaround for establishing REST resources.",
    "bower-asset/angular-route": "Enables routing inside the AngularJS application."
  }
}